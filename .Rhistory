colData = immuno.conds,
design = ~ conditions_p)
dds.up <- DESeq(dds.up)
res.up <- results(dds.up)
resup<-list(resu=res.up)
immuno.data.out.u <- list(resup)
save(immuno.data.out.u, file="./analysis/immuno.data.u.deseq.Rda")
#unpermuted BRCA
